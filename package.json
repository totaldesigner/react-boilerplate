{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "main": "build/server/index.js",
  "license": "MIT",
  "pre-commit": "lint",
  "scripts": {
    "clean": "rimraf build",
    "dev": "npm run clean && cross-env NODE_ENV=development npm run watch:server",
    "build": "npm run clean && npm run build:server && npm run build:app && npm run build:serverRenderer",
    "build:app": "webpack --config ./webpack.app.prod.js -p",
    "build:serverRenderer": "webpack --config ./webpack.server.js -p",
    "build:server": "tsc -p tsconfig-server.json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:style": "stylelint app/components/**/*.scss app/containers/**/*.scss app/styles.scss",
    "lint:ts": "tslint app/**/*.tsx app/**/*.ts server/**/*.tsx server/**/*.ts test/**/*.tsx test/**/*.ts",
    "start": "nodemon",
    "start:prod": "cross-env NODE_ENV=production node build/server/index.js",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "keywords": [
    "React",
    "TypeScript",
    "SSR"
  ],
